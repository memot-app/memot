---
import type { HTMLAttributes } from "astro/types";
import AppLinkWeb from "../AppLink/AppLinkWeb.astro";
import BrandLogoFull from "../Brand/BrandLogoFull.astro";

interface Props extends HTMLAttributes<"header"> {}

const { class: className, ...props } = Astro.props;
---

<header
  class:list={["flex items-center justify-between", className]}
  {...props}
>
  <BrandLogoFull class="h-[45px]" />
  <AppLinkWeb class="h-[48px]" />
</header>
